{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "description": "A SaaS",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --open",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test": "vue-cli-service storybook --debug-webpack"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@fullcalendar/core": "5.10.0",
    "@fullcalendar/daygrid": "5.10.0",
    "@fullcalendar/interaction": "5.10.0",
    "@fullcalendar/timegrid": "5.10.0",
    "@fullcalendar/vue": "5.10.0",
    "axios": "^0.21.1",
    "chart.js": "2.9.4",
    "chartjs-plugin-annotation": "^0.5.7",
    "core-js": "3.26.0",
    "d3": "7.1.1",
    "d3-org-chart": "^1.1.3",
    "datamaps": "0.5.9",
    "debounce": "^1.2.1",
    "element-ui": "2.15.6",
    "google-maps": "4.3.3",
    "jwt-decode": "^3.1.2",
    "nprogress": "0.2.0",
    "perfect-scrollbar": "1.5.2",
    "register-service-worker": "1.7.2",
    "sweetalert2": "11.1.8",
    "topojson": "3.0.2",
    "v-suggest": "^2.2.0",
    "vee-validate": "^3.4.14",
    "vue": "2.6.14",
    "vue-chartjs": "3.5.1",
    "vue-clickaway": "2.2.2",
    "vue-cookies": "^1.7.4",
    "vue-feather-icons": "^5.1.0",
    "vue-notifyjs": "0.4.3",
    "vue-router": "3.5.2",
    "vue-router-prefetch": "^1.6.0",
    "vue-star-rating": "^1.7.0",
    "vue-suggestion": "^2.0.0",
    "vue-sweetalert2": "^4.2.1",
    "vue2-datepicker": "^3.10.4",
    "vue2-timepicker": "^1.1.6",
    "vue2-transitions": "0.3.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/builder-webpack5": "^6.3.0",
    "@storybook/manager-webpack5": "^6.3.0",
    "@storybook/vue": "^6.0.26",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-component": "1.1.1",
    "css-loader": "^6.7.1",
    "eslint": "~8.11.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "eslint-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "prettier": "^2.5.1",
    "sass": "~1.43.2",
    "sass-loader": "12.0.0",
    "vue-cli-plugin-storybook": "~2.1.0",
    "vue-loader": "^15.10.0",
    "vue-template-compiler": "2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "plugins": [
      "import"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ]
          ],
          "extensions": [
            ".vue",
            ".json",
            ".js"
          ]
        }
      }
    },
    "rules": {
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "vue": "never",
          "js": "never",
          "jsx": "never"
        }
      ],
      "template-curly-spacing": [
        "error",
        "never"
      ],
      "vuejs-accessibility/form-control-has-label": 0,
      "vuejs-accessibility/label-has-for": [
        "error",
        {
          "required": {
            "every": [
              "id"
            ]
          },
          "allowChildren": false
        }
      ],
      "vue/multi-word-component-names": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "vueIndentScriptAndStyle": true,
    "printWidth": 100
  },
  "browserslist": [
    "defaults and supports es6-module",
    "> 1%",
    "last 2 versions",
    "not ie < 10"
  ],
  "engines": {
    "node": ">= 13.0.0",
    "npm": ">= 6.0.2"
  }
}
